{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "CRUD API using in-memory database",
  "main": "src/server.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start:prod": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "start:dev": "npm run dev",
    "test": "jest --watch",
    "start:multi": "npm run build && ts-node src/loadBalancer.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SvitlanaG/CRUD-API.git"
  },
  "keywords": [
    "crud"
  ],
  "author": "Svitlana Grytsai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SvitlanaG/CRUD-API/issues"
  },
  "homepage": "https://github.com/SvitlanaG/CRUD-API#readme",
  "dependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.7",
    "@types/uuid": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "uuid": "^10.0.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
